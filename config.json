{
  "name": "FHEM (local)",
  "version": "0.1.0",
  "slug": "fhem_local",
  "description": "FHEM Plugin zalc1 fork",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/zalc1/addon-fhem",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8083]/fhem",
  "panel_icon": "mdi:HomeFloorL",
  "panel_title": "FHEM",
  "uart": true,
  "gpio": false,
  "ingress": false,
  "apparmor": false,
  "ports": {
    "8083/tcp": 8083,
    "7072/tcp": 7072
  },
  "ports_description": {
    "8083/tcp": "WebUI",
    "7072/tcp": "telnet"
  },
  "map": ["config:rw"],
  "host_network": false,
  "environment": {
  },
  "devices": [
  ],
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "init": false,
  "options": {
    "apt_pks": "vim libjson-perl liblwp-protocol-https-perl libxml-simple-perl",
    "pip_pks": "",
    "npm_pks": "",
    "cpan_pks": "Net::MQTT Net::MQTT::Message Net::MQTT::Message::JustMessageId Readonly"
  },
  "schema": {
    "apt_pks": "str",
    "pip_pks": "str",
    "npm_pks": "str",
    "cpan_pks": "str"
  }

}
